{% macro bagItem(title, price, image, info, part_no, sid) %}
    <div class="item-pod">
        <h5 class="line-clamp-2 title">
            {{ title }}
        </h5>
        <div class="details">
            <div class="image-copy-wrapper">
                <div class="image-wrapper">
                    <a href="{{ image }}" title="{{ title }}" class="lc_lightbox_link image-link">
                        <img src="{{ image }}" alt="{{ title }}">
                    </a>
                    <div class="flex flex-col gap-0 text-sm">
                        <span>Part No:</span>
                        <strong>{{ part_no }}</strong>
                    </div>
                </div>
                <div class="copy-wrapper">
                    <div class="copy">
                        {{ info }}
                    </div>
                </div>
            </div>
            <div class="action">
                <div class="price-wrapper">
                    <span class="lbl">Cost per unit:</span>
                    <span class="val">£{{ price }}</span>
                </div>
                <form class="single-input-form form">
                    {{ quantityField("quantity-" ~ part_no) }}
                    <input type="hidden" class="sid" name="sid" value="{{ sid }}">
                    <input type="hidden" class="price" name="price" value="{{ price }}">
                    <button type="button" class="bttn btn_addBasketAll">
                        <span class="icon">
                            <i class="fa-solid fa-cart-shopping"></i>
                        </span>
                        <span>Add</span>
                    </button>
                </form>
            </div>
        </div>
    </div>
{% endmacro %}

